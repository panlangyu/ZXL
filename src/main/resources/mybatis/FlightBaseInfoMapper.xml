<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="FlightBaseInfoMapper">
	<!--根据出发地三字码查询出发机场三字码  -->
	<select id="getDepApcodeByCtcode" parameterType="string" resultType="java.lang.String">
		SELECT FUNC_GETAIRPORT(#{depCode}) FROM DUAL  
	</select>
	<!--根据到达地三字码查询到达机场三字码  -->
	<select id="getDesApcodeByCtcode" parameterType="string" resultType="java.lang.String">
		SELECT FUNC_GETAIRPORT(#{desCode}) FROM DUAL 
	</select>	
</mapper>